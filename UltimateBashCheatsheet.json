{
"bash\_commands": \[
{"command": "ls", "description": "List directory contents", "examples": \["ls", "ls -l", "ls -a /home/user"]},
{"command": "cd", "description": "Change directory", "examples": \["cd /home/user", "cd ..", "cd \~"]},
{"command": "pwd", "description": "Print current working directory", "examples": \["pwd"]},
{"command": "mkdir", "description": "Create directories", "examples": \["mkdir new\_folder", "mkdir -p /tmp/new\_folder/subfolder"]},
{"command": "rm", "description": "Remove files or directories", "examples": \["rm file.txt", "rm -r old\_folder", "rm -f file.txt"]},
{"command": "cp", "description": "Copy files or directories", "examples": \["cp file1.txt file2.txt", "cp -r folder1/ folder2/"]},
{"command": "mv", "description": "Move or rename files or directories", "examples": \["mv oldname.txt newname.txt", "mv file.txt /tmp/"]},
{"command": "echo", "description": "Display a line of text", "examples": \["echo 'Hello World'", "echo \$HOME"]},
{"command": "cat", "description": "Concatenate and display file content", "examples": \["cat file.txt", "cat file1.txt file2.txt"]},
{"command": "grep", "description": "Search text using patterns", "examples": \["grep 'hello' file.txt", "grep -i 'hello' file.txt", "grep -r 'pattern' /path/to/dir"]},
{"command": "awk", "description": "Pattern scanning and processing language", "examples": \["awk '{print \$1}' file.txt", "awk -F, '{print \$2}' file.csv"]},
{"command": "sed", "description": "Stream editor for text transformations", "examples": \["sed 's/old/new/g' file.txt", "sed -n '1,5p' file.txt"]},
{"command": "find", "description": "Search for files in directory hierarchy", "examples": \["find /home -name file.txt", "find . -type d -name 'folder\*'"]},
{"command": "chmod", "description": "Change file permissions", "examples": \["chmod 755 script.sh", "chmod +x script.sh"]},
{"command": "chown", "description": "Change file owner and group", "examples": \["chown user\:group file.txt", "chown -R user\:group /home/user/folder"]},
{"command": "tar", "description": "Archive files", "examples": \["tar -cvf archive.tar file1 file2", "tar -xvf archive.tar", "tar -czvf archive.tar.gz folder/"]},
{"command": "zip", "description": "Compress files into zip archive", "examples": \["zip archive.zip file1 file2", "zip -r archive.zip folder/"]},
{"command": "unzip", "description": "Extract files from zip archive", "examples": \["unzip archive.zip", "unzip archive.zip -d /tmp"]},
{"command": "wget", "description": "Download files from the web", "examples": \["wget [http://example.com/file.txt](http://example.com/file.txt)", "wget -O newfile.txt [http://example.com/file.txt](http://example.com/file.txt)"]},
{"command": "curl", "description": "Transfer data from or to a server", "examples": \["curl [http://example.com](http://example.com)", "curl -O [http://example.com/file.txt](http://example.com/file.txt)"]},
{"command": "ps", "description": "Display process status", "examples": \["ps", "ps aux", "ps -ef"]},
{"command": "top", "description": "Display dynamic process information", "examples": \["top"]},
{"command": "htop", "description": "Interactive process viewer", "examples": \["htop"]},
{"command": "kill", "description": "Terminate processes", "examples": \["kill 1234", "kill -9 1234"]},
{"command": "jobs", "description": "List background jobs", "examples": \["jobs"]},
{"command": "fg", "description": "Bring background job to foreground", "examples": \["fg %1"]},
{"command": "bg", "description": "Send job to background", "examples": \["bg %1"]},
{"command": "alias", "description": "Create command shortcuts", "examples": \["alias ll='ls -l'", "alias gs='git status'"]},
{"command": "unalias", "description": "Remove aliases", "examples": \["unalias ll"]},
{"command": "history", "description": "Show command history", "examples": \["history", "history | grep ls"]},
{"command": "man", "description": "Show manual of commands", "examples": \["man ls", "man grep"]},
{"command": "which", "description": "Locate command executable", "examples": \["which python", "which ls"]},
{"command": "whereis", "description": "Locate binary, source, and manual", "examples": \["whereis ls", "whereis python"]},
{"command": "uname", "description": "Show system information", "examples": \["uname", "uname -a"]},
{"command": "df", "description": "Show disk space usage", "examples": \["df", "df -h"]},
{"command": "du", "description": "Estimate file space usage", "examples": \["du file.txt", "du -h /home/user"]},
{"command": "mount", "description": "Mount filesystems", "examples": \["mount /dev/sdb1 /mnt", "mount -t ext4 /dev/sdb1 /mnt"]},
{"command": "umount", "description": "Unmount filesystems", "examples": \["umount /mnt"]},
{"command": "scp", "description": "Secure copy files over SSH", "examples": \["scp file.txt user\@host:/tmp/", "scp -r folder/ user\@host:/tmp/"]},
{"command": "ssh", "description": "Secure shell login", "examples": \["ssh user\@host", "ssh -p 2222 user\@host"]},
{"command": "rsync", "description": "Sync files locally or remotely", "examples": \["rsync -av /src/ /dest/", "rsync -avz /local user\@remote:/backup/"]},
{"command": "cron", "description": "Schedule recurring tasks", "examples": \["crontab -e", "0 5 \* \* \* /home/user/backup.sh"]},
{"command": "crontab", "description": "Manage user cron jobs", "examples": \["crontab -l", "crontab -r"]},
{"command": "systemctl", "description": "Control system services", "examples": \["systemctl status nginx", "systemctl restart nginx"]},
{"command": "journalctl", "description": "View system logs", "examples": \["journalctl -u nginx", "journalctl -xe"]},
{"command": "df", "description": "Check disk usage", "examples": \["df -h", "df -T"]},
{"command": "free", "description": "Display memory usage", "examples": \["free", "free -h"]},
{"command": "uptime", "description": "Show how long the system has been running", "examples": \["uptime"]},
{"command": "dmesg", "description": "Print kernel messages", "examples": \["dmesg", "dmesg | tail"]},
{"command": "ip", "description": "Show/manipulate routing, devices, policy routing and tunnels", "examples": \["ip addr", "ip link set eth0 up"]},
{"command": "ping", "description": "Send ICMP ECHO\_REQUEST to network hosts", "examples": \["ping google.com", "ping -c 4 google.com"]},
{"command": "traceroute", "description": "Trace route to host", "examples": \["traceroute google.com"]},
{"command": "netstat", "description": "Network statistics", "examples": \["netstat -tulnp"]},
{"command": "ss", "description": "Socket statistics", "examples": \["ss -tulw"]},
{"command": "iptables", "description": "Configure Linux kernel firewall", "examples": \["iptables -L", "iptables -A INPUT -p tcp --dport 22 -j ACCEPT"]}
],

"bash\_operators": \[
{"operator": ";", "description": "Run commands sequentially", "examples": \["cd /tmp; ls"]},
{"operator": "&&", "description": "Run second command only if first succeeds", "examples": \["mkdir new\_folder && cd new\_folder"]},
{"operator": "||", "description": "Run second command only if first fails", "examples": \["mkdir existing\_folder || echo 'Folder exists'"]},
{"operator": "&", "description": "Run command in background", "examples": \["sleep 10 &"]},
{"operator": "|", "description": "Pipe output to another command", "examples": \["ls -l | grep '.txt'"]},
{"operator": ">", "description": "Redirect stdout to file", "examples": \["echo 'Hello' > file.txt"]},
{"operator": ">>", "description": "Append stdout to file", "examples": \["echo 'World' >> file.txt"]},
{"operator": "<", "description": "Redirect stdin from file", "examples": \["wc -l < file.txt"]},
{"operator": "2>", "description": "Redirect stderr to file", "examples": \["ls /nonexistent 2> error.log"]},
{"operator": "&>", "description": "Redirect stdout and stderr to file", "examples": \["command &> output.log"]},
{"operator": "\$()", "description": "Command substitution", "examples": \["echo 'Today is \$(date)'"]},
{"operator": "*", "description": "Wildcard: match zero or more characters", "examples": \["ls *.txt"]},
{"operator": "?", "description": "Wildcard: match exactly one character", "examples": \["ls file?.txt"]},
{"operator": "\[]", "description": "Wildcard: match any one character in brackets", "examples": \["ls file\[1-3].txt"]},
{"operator": "{}", "description": "Brace expansion", "examples": \["echo file{1,2,3}.txt"]},
{"operator": "\~", "description": "Home directory shortcut", "examples": \["cd \~", "ls \~/Documents"]},
{"operator": "\$", "description": "Variable expansion", "examples": \["echo \$HOME", "name='John'; echo \$name"]},
{"operator": "()", "description": "Subshell", "examples": \["(cd /tmp; ls)"]},
{"operator": "{}", "description": "Command grouping", "examples": \["{ echo one; echo two; }"]},
{"operator": "!", "description": "Logical NOT or history expansion", "examples": \["!ls", "if ! \[ -f file ]; then echo 'Missing'; fi"]},
{"operator": "=", "description": "Assignment", "examples": \["var=10", "name='John'"]},
{"operator": "+=", "description": "Append to variable", "examples": \["var+=5", "list+=' item'"]},
{"operator": "%", "description": "Modulo operator in arithmetic", "examples": \["echo \$((5%2))"]},
{"operator": "*/+-", "description": "Arithmetic operators", "examples": \["echo \$((3+2))", "echo \$((4*5))"]}
]
}
